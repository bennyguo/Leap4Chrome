{
  "manifest_version": 2,
  "name": "Leap4Chrome",
  "version": "0.9",
  "content_scripts": [
	  {
	    "matches": [
	      "<all_urls>"
	    ],
	    "js": ["jquery.js", "leap-0.6.4.js", "leaptrainer.js", "leap-plugins-0.1.12.js", "cross-correlation.js", "content.js"]
	  }
  ],
  "browser_action": {
  	"default_icon": "icon.png"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
    "tabs",
    "http://*/",
    "https://*/"
  ]
}